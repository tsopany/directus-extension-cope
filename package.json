{
	"name": "directus-extension-cope",
	"description": "CoPe: Sync Directus Configs",
	"icon": "extension",
	"version": "1.0.2",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-hook"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"l": "pnpm up -i -L"
	},
	"dependencies": {
		"@directus/extensions": "^3.0.19",
		"@types/fs-extra": "^11.0.4",
		"fs-extra": "^11.3.3"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "17.0.9",
		"@types/node": "^25.2.3",
		"typescript": "^5.9.3"
	},
	"packageManager": "pnpm@10.29.2"
}
